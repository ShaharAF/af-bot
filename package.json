{
  "name": "af-bot",
  "version": "1.0.0",
  "description": "a friendly bot to help you with submitting your issue to AppsFlyer ",
  "author": "Shahar Cohen <shahar.cohen@appsflyer.com>",
  "license": "ISC",
  "repository": "https://github.com//af-bot.git",
  "homepage": "https://github.com//af-bot",
  "bugs": "https://github.com//af-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/robot.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "js-yaml": "~3.13.1",
    "probot": "^9.3.1",
    "probot-scheduler": "~2.0.0-beta.1"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
